{
    "name": "rainsens/dinglog",
    "description": "Send laravel log to DingTalk.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rainsens\\Dinglog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rainsens\\Dinglog\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "rainsen",
            "email": "yusen@rainsen.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-bcmath": "*",
        "php": ">=7.1.0",
        "laravel/framework": ">=5.5",
        "alibabacloud/dingtalk": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rainsens\\Dinglog\\Providers\\DinglogServiceProvider"
            ],
            "aliases": {
                "Dinglog": "Rainsens\\Dinglog\\Facades\\Dinglog"
            }
        }
    }
}
